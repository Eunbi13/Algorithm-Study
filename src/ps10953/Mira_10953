import java.util.Scanner;
import java.util.StringTokenizer;

public class Mira_10953 {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        StringTokenizer st;     
        String str;
        
        int T = sc.nextInt();  // 입력 값 받아옴
        
        for(int i=0; i<T; i++){  // i가 0부터 시작하여 T보다 작을때까지 반복 (첫 입력값이 5 이므로 5번 반복)
        
            str = sc.next();                     // next = 문자 또는 문자열을 공백 기준으로 한단어 또는 한 문자씩 입력받는다.
            
            st = new StringTokenizer(str, ",");  // StringTokenizer = 사용자가 지정하는 구분자를 경계로 하여 문자열을 나눠주는 Class. 
                                                 // 만약 사용자가 구분자(,.=) 지정을 생략하면 공백이나 탭이 기본 구분자로 사용된다.
                                                 // 콤마기준으로 string 분리하기 위해 사용
            
            int a = Integer.parseInt(st.nextToken());   // Integer.parseInt(String) = int 형을 리턴하기 때문에 산술 연산을 할 수 있다.
            int b = Integer.parseInt(st.nextToken());   // parseInt = 문자열을 정수로 바꾸는 함수 
                                                        // nextToken = StringTokenizer 클래스 객체에서 다음 토큰을 읽어 들인다. 
                                                        // st 안에 있는 토큰을 읽어와서 a에 정수로 담음
            
            System.out.println(a+b);
        }
        sc.close();
    }   
}

// < StringTokenizer 과 함께 하용 할수 있는 메소드 > 
// hasMoreTokens() = StringTokenizer 클래스 객체에서 다음에 읽어 들일 token이 있으면 true, 없으면 false를 return한다.
// nextToken() = StringTokenizer 클래스 객체에서 다음 토큰을 읽어 들인다.

